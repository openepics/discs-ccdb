<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/beans_1_0.xsd">
    <alternatives>
        <class>org.openepics.discs.conf.util.AppPropertiesJBoss</class>
        <class>org.openepics.discs.conf.util.names.NamesForDevelopment</class>
        <!-- To get list of names from ESS naming convention tool, replace upper
         alternative with an alternative below.
         <class>org.openepics.discs.conf.util.names.NamesESS</class>     
         -->                

		<!-- To Enable RBAC Replace with the alternative bellow 
        <class>org.openepics.discs.conf.security.RBACEntityTypeSecurityPolicy</class>
        -->
    </alternatives>

    <interceptors>
        <class>org.openepics.discs.conf.security.AuthorizationInterceptor</class>
        <class>org.openepics.discs.conf.auditlog.AuditInterceptor</class>
    </interceptors>
</beans>
