<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>CUSTOM-Release</actionName>
            <displayName>Release</displayName>
            <goals>
                <goal>deploy</goal>
            </goals>
            <activatedProfiles>
                <activatedProfile>release</activatedProfile>
            </activatedProfiles>
        </action>
        <action>
            <actionName>CUSTOM-Deploy: No tests</actionName>
            <displayName>Deploy: No tests</displayName>
            <goals>
                <goal>deploy</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-Clean Install No-Tests</actionName>
            <displayName>Clean Install No-Tests</displayName>
            <goals>
                <goal>clean</goal>
                <goal>install</goal>
            </goals>
            <properties>
                <skipTests>true</skipTests>
            </properties>
        </action>
    </actions>
